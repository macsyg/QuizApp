<html lang="pl">
    <head>
        <meta charset="utf-8">
        <link rel="stylesheet" href="css/add_quiz.css" />
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script src="js/add_quiz.js"></script>
    </head>
    <body>
        <form id="center-form" action="/add_quiz" method="POST">
            <div id='quiz'>
                <div id="info">
                    <div class="field-info">
                        <input type="hidden" id="qval" name="qval" value=0>
                        <label for="title">Title</label>
                        <input type="text" placeholder="title" name="title" required>
                        <!-- <br> -->
                        <label for="type">Choose type:</label>
                        <select id="type" name="typeslist">
                            <option value="community">Community</option>
                            <option value="private">Private</option>
                        </select>
                    </div>
                </div>
            </div>
            <div id='buttons'>
                <button class="center-button" id="add_q" type="button">Add question</button>
                <button class="center-button" type="submit">Add quiz</button>
                <button class="center-button" id="leave-button" type="button">Leave creator</button>
            </div>
        </form>
        <!-- <button onclick="addQuestion(); return false;">Add question</button> -->
        <!-- <button class="center-button" id="leave-button">Leave creator</button> -->
    </body>
</html>